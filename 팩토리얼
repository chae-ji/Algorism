import java.util.Scanner;

public class Main {

    static int fac(int num) {
        if (num <= 1) {
            return 1;
        } else
            return num * fac(num - 1);
    }
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int fac = fac(num);
        System.out.println(fac);
    }

}
